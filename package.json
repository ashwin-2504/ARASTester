{
  "name": "arastester",
  "version": "1.0.0",
  "description": "This is a application for automatic testing of ARAS",
  "license": "ISC",
  "author": "Gopale Ashwin",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "build:backend": "cd backend/ArasBackend && dotnet build -c Debug",
    "build:backend:prod": "cd backend/ArasBackend && dotnet publish -c Release -o ../../dist-backend",
    "predev": "npm run build:backend",
    "dev": "node dev-runner.js",
    "dev:server": "vite",
    "dev:electron": "electron . --dev",
    "build": "vite build",
    "start": "npm run build && electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.5.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.10"
  }
}